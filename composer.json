{
    "name": "dragonrun1/event-mediator",
    "description": "A general event mediator (dispatcher) which has minimal dependencies so it is easy to drop in and use.",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Michael Cummings",
            "email": "mgcummings@yahoo.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-0": {
            "Spec\\": "specs/"
        },
        "psr-4": {
            "EventMediator\\": [
                "src/"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.*-dev"
        }
    },
    "homepage": "https://github.com/Dragonrun1/event-mediator",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ext-SPL": "*",
        "php": ">=7.0, <7.2",
        "pimple/pimple": "~3.0"
    },
    "require-dev": {
        "henrikbjorn/phpspec-code-coverage": "dev-master",
        "phpspec/nyan-formatters": "~2.0.0",
        "phpspec/phpspec": "~3.0.0",
        "phpunit/php-code-coverage": "~4.0",
        "satooshi/php-coveralls": "dev-master",
        "symfony/console": "~2.8"
    },
    "replace": {
        "dragonrun1/event-mediator": "2.0.*"
    }
}
